<div>
<% if (Object.keys(data.inventory).length === 0) { %>
    <p>You don't have anything.</p>
<% } else { %>
    <table class="table table-bordered table-condensed">
        <tr>
            <th>Name</th>
            <th>Content</th>
        </tr>
        <% for (item of Object.keys(data.inventory)) {%>
            <tr>
                <td>
                    <%= item %>
                </td>
                <td>
                    <%= data.inventory[item] %>
                </td>
                <td>
                    <a class="btn btn-primary" href="/editor/<%=item%>/<%=data.inventory[item]%>">Edit</a>
                    <a class="btn btn-danger" href="/delete/<%=item%>">Delete</a>
                </td>
            </tr>
            <%}%>
    </table>
<% } %>
</div>
<div>
    <a class="btn btn-primary" href="/editor/%20/%20">Create new entry</a>
    <% if (uid !== 'j:null') {%>
        <p class="fw-light">Your current UID: <%= uid %> </p>
    <% } %>
</div>